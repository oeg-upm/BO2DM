# http://bimerr.iot.linkeddata.es/def/building#Building

building:Building a owl:Class ;
	rdfs:subClassOf s4bldg:Building , _:genid1 .

_:genid1 a owl:Restriction ;
	owl:onProperty :buildingAddress ;
	owl:someValuesFrom xsd:string .

building:Building rdfs:subClassOf _:genid2 .

_:genid2 a owl:Restriction ;
	owl:onProperty :description ;
	owl:someValuesFrom xsd:string .

building:Building rdfs:subClassOf _:genid3 .

_:genid3 a owl:Restriction ;
	owl:onProperty :identifier ;
	owl:someValuesFrom xsd:string .

building:Building rdfs:comment "Building where the occupancy behaviors occur."@en ;
	rdfs:label "Building"@en .
# 
# http://bimerr.iot.linkeddata.es/def/building#BuildingSpace

building:BuildingSpace a owl:Class ;
	rdfs:subClassOf s4bldg:BuildingSpace , _:genid4 .

_:genid4 a owl:Restriction ;
	owl:onProperty :hasMeeting ;
	owl:allValuesFrom :Meeting .

building:BuildingSpace rdfs:subClassOf _:genid5 .

_:genid5 a owl:Restriction ;
	owl:onProperty :hasOccupant ;
	owl:allValuesFrom :Occupant .

building:BuildingSpace rdfs:subClassOf _:genid6 .

_:genid6 a owl:Restriction ;
	owl:onProperty :identifier ;
	owl:someValuesFrom xsd:string .

building:BuildingSpace rdfs:subClassOf _:genid7 .

_:genid7 a owl:Restriction ;
	owl:onProperty :description ;
	owl:allValuesFrom xsd:string .

building:BuildingSpace rdfs:subClassOf _:genid8 .

_:genid8 a owl:Restriction ;
	owl:onProperty :maxNumberOccupants ;
	owl:allValuesFrom xsd:integer .

building:BuildingSpace rdfs:subClassOf _:genid9 .

_:genid9 a owl:Restriction ;
	owl:onProperty :minNumberOccupants ;
	owl:allValuesFrom xsd:integer .

building:BuildingSpace rdfs:comment "Class to represent the different spaces the building have and where the occupant behaviors occur." ;
	rdfs:label "Building Space"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#AccousticNeed

:AccousticNeed a owl:Class ;
	rdfs:subClassOf :PhysicalNeed ;
	rdfs:comment "Range of parameter values where occupants do not feel accoustic discomfort and therefore, they don't carry out any actions."@en ;
	rdfs:label "Accoustic Need"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Action

:Action a owl:Class ;
	rdfs:comment "Actions are the interactions with systems or activities that an occupant can conduct to achieve environmental comfort. Actions connect occupants' inside-world needs with the environmental outside world. (Defintion taken from obXML)"@en ;
	rdfs:label "Action"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Behavior

:Behavior a owl:Class ;
	rdfs:subClassOf _:genid10 .

_:genid10 a owl:Restriction ;
	owl:onProperty :drivenBy ;
	owl:someValuesFrom :DriverSet .

:Behavior rdfs:subClassOf _:genid11 .

_:genid11 a owl:Restriction ;
	owl:onProperty :leadsTo ;
	owl:someValuesFrom _:genid12 .

_:genid12 a owl:Class ;
	owl:unionOf _:genid14 .

_:genid14 a rdf:List ;
	rdf:first :Action ;
	rdf:rest _:genid13 .

_:genid13 a rdf:List ;
	rdf:first :Inaction ;
	rdf:rest rdf:nil .

:Behavior rdfs:subClassOf _:genid15 .

_:genid15 a owl:Restriction ;
	owl:onProperty :influencedByNeed ;
	owl:allValuesFrom :Need .

:Behavior rdfs:subClassOf _:genid16 .

_:genid16 a owl:Restriction ;
	owl:onProperty :identifier ;
	owl:someValuesFrom xsd:string .

:Behavior rdfs:subClassOf _:genid17 .

_:genid17 a owl:Restriction ;
	owl:onProperty :description ;
	owl:allValuesFrom xsd:string .

:Behavior rdfs:comment "Behavior related to the interaction with buildings"@en ;
	rdfs:label "Behavior"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Coefficient

:Coefficient a owl:Class ;
	rdfs:subClassOf _:genid18 .

_:genid18 a owl:Restriction ;
	owl:onProperty :coefficientValue ;
	owl:someValuesFrom xsd:float .

:Coefficient rdfs:subClassOf _:genid19 .

_:genid19 a owl:Restriction ;
	owl:onProperty :name ;
	owl:someValuesFrom xsd:string .

:Coefficient rdfs:comment "Class to represent the value and name of coefficients inside mathematical formulas."@en ;
	rdfs:label "Coefficient"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#ConstantValueFormula

:ConstantValueFormula a owl:Class ;
	rdfs:subClassOf :InteractionFormula ;
	rdfs:comment "Class to represent constant value mathematical formulas"@en ;
	rdfs:label "Constant Value Formula"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#CustomProbabilityModel

:CustomProbabilityModel a owl:Class ;
	rdfs:subClassOf :MovementModel , _:genid20 .

_:genid20 a owl:Restriction ;
	owl:onProperty :probability ;
	owl:someValuesFrom xsd:float .

:CustomProbabilityModel rdfs:subClassOf _:genid21 .

_:genid21 a owl:Restriction ;
	owl:onProperty :time ;
	owl:someValuesFrom xsd:time .

:CustomProbabilityModel rdfs:comment "Class to represent instances of custom probabilistic models."@en ;
	rdfs:label "Custom probability model"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Driver

:Driver a owl:Class ;
	rdfs:comment "A Driver represents the environmental factor from the outside world that stimulate occupants in their inside world to fulfill a physical, physiological or psychological need."@en ;
	rdfs:label "Driver"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#DriverSet

:DriverSet a owl:Class ;
	rdfs:subClassOf _:genid22 .

_:genid22 a owl:Restriction ;
	owl:onProperty :composedOf ;
	owl:someValuesFrom :Driver .

:DriverSet rdfs:comment "Set of drivers that could be causing a behavior at a specific time."@en ;
	rdfs:label "Driver set"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Environment

:Environment a owl:Class ;
	rdfs:subClassOf :Driver , _:genid23 .

_:genid23 a owl:Restriction ;
	owl:onProperty :hasEnvironmentalParameter ;
	owl:someValuesFrom :EnvironmentalParameter .

:Environment rdfs:comment "Driver related to the environmental conditions that could be causing a behavior."@en ;
	rdfs:label "Environment"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#EnvironmentalParameter

:EnvironmentalParameter a owl:Class ;
	rdfs:subClassOf _:genid24 .

_:genid24 a owl:Restriction ;
	owl:onProperty :describesProperty ;
	owl:someValuesFrom saref:Property .

:EnvironmentalParameter rdfs:subClassOf _:genid25 .

_:genid25 a owl:Restriction ;
	owl:onProperty :hasUnitOfMeasure ;
	owl:someValuesFrom saref:UnitOfMeasure .

:EnvironmentalParameter rdfs:subClassOf _:genid26 .

_:genid26 a owl:Restriction ;
	owl:onProperty :identifier ;
	owl:someValuesFrom xsd:string .

:EnvironmentalParameter rdfs:subClassOf _:genid27 .

_:genid27 a owl:Restriction ;
	owl:onProperty :description ;
	owl:allValuesFrom xsd:string .

:EnvironmentalParameter rdfs:subClassOf _:genid28 .

_:genid28 a owl:Restriction ;
	owl:onProperty :name ;
	owl:allValuesFrom xsd:string .

:EnvironmentalParameter rdfs:comment "Class that represent a physical variable of the environment where the driver occurs. It could represent variables that impact directly on the comfort levels of the occupant."@en ;
	rdfs:label "Environmental parameter"@en .
# 
# http://bimerr.iot.linkeddata.es/def/occupancy-profile#Equipment

:Equipment a owl:Class ;
